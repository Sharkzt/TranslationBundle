{
    "name": "vkr/translation-bundle",
    "type": "symfony-bundle",
    "description": "Translates texts from DB for non-annotation mappings, includes integration with Google Translate",
    "keywords": ["translation", "doctrine", "symfony2", "symfony3"],
    "homepage": "https://github.com/wladislavk/TranslationBundle",
    "license": "MIT",
    "authors": [
        {"name": "Vladislav Kryshtanovskiy", "email": "wladislav.k@gmail.com"}
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "~2.8|~3.0",
        "doctrine/orm": ">=2.2.3",
        "doctrine/doctrine-bundle": "~1.4",
        "google/cloud": "^0.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "VKR\\TranslationBundle\\": ""
        }
    }
}
